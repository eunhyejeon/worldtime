<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="og-bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFF3E0;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#FFF8E1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFFDE7;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="1200" height="630" fill="url(#og-bg)"/>
  
  <!-- Background circles -->
  <circle cx="200" cy="150" r="80" fill="#FFB74D" opacity="0.1"/>
  <circle cx="1000" cy="480" r="120" fill="#FF8A65" opacity="0.1"/>
  <circle cx="600" cy="100" r="60" fill="#FFA726" opacity="0.1"/>
  
  <!-- Main content -->
  <g transform="translate(600, 315)">
    <!-- Clock icon -->
    <circle cx="0" cy="-50" r="60" fill="white" stroke="#FFB74D" stroke-width="4"/>
    <circle cx="0" cy="-50" r="50" fill="white"/>
    <circle cx="0" cy="-50" r="3" fill="#E65100"/>
    <line x1="0" y1="-50" x2="0" y2="-80" stroke="#E65100" stroke-width="3" stroke-linecap="round"/>
    <line x1="0" y1="-50" x2="25" y2="-50" stroke="#E65100" stroke-width="2" stroke-linecap="round"/>
  </g>
  
  <!-- Title -->
  <text x="600" y="400" text-anchor="middle" font-family="Arial, sans-serif" font-size="48" font-weight="bold" fill="#E65100">World Time Checker</text>
  
  <!-- Subtitle -->
  <text x="600" y="450" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" fill="#666">Track time zones from around the world</text>
  
  <!-- Farcaster badge -->
  <rect x="500" y="500" width="200" height="40" rx="20" fill="#FFB74D"/>
  <text x="600" y="525" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white">Farcaster Mini App</text>
  
  <!-- Globe emoji -->
  <text x="600" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="60">🌍</text>
</svg>